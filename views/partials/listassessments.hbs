<h2 class="ui header">
    {{title}}
  </h2>
<table class="ui table">
  <thead>
    
    <tr>
      <th>Weight </th>
      <th>Chest</th>
      <th>Thigh</th>
      <th>Upper Arm</th>
      <th>Waist</th>
      <th>Hips</th>
      <th>Comments</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  
    {{#each assessmentlist}}
    {{#each assessmentResults}}
       
    
      <tr>
            <td>
              {{weight}}
            </td>
            <td>
              {{chest}}
            </td>
            <td>
              {{thigh}}
            </td>
            <td>
              {{upperarm}}
            </td>
            <td>
              {{waist}}
            </td>
            <td>
              {{hips}}
            </td>
            <td>
              {{comments}}
            </td>
            <td>
            <a href="/assessment/{{../id}}/deleteAssessment/{{id}}" class="ui icon button">
            <i class="icon trash"></i>
            </a>
            </td>        
      </tr>
    {{/each}} 
    {{/each}}  
  </tbody>
</table>